#Write a Python function that accepts a string and calculates the number of upper case letters and lower case letters.
def up_low(s):
  u=0
  l=0
  for i in range(len(s)):
    if s[i].isalpha(): 
      if s[i]==s[i].upper():
        u+=1
      else:
        l+=1
  print(f'Original: {s}')
  print(f'Lower case letters: {l}')
  print(f'Upper case letters: {u}')
